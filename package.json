{
  "name": "internet-speed-monitor",
  "version": "1.0.0",
  "description": "Internet speed tester application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . internet-speed-monitor --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . internet-speed-monitor --overwrite --asar --platform=win32 --arch=ia32 --icon=resource/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Internet speed monitor\"",
    "package-linux": "electron-packager . internet-speed-monitor --overwrite --asar=true --platform=linux --arch=x64 --icon=resource/icon.png --prune=true --out=release-builds"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [],
  "author": "Rigoberto L. Salgado Reyes",
  "license": "AGPL-3.0",
  "devDependencies": {
    "electron": "^19.0.9",
    "electron-packager": "^15.5.1"
  },
  "dependencies": {
    "sqlite3": "^5.0.10",
    "universal-speedtest": "^2.0.5"
  }
}
