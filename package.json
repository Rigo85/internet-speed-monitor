{
  "name": "internet-speed-monitor",
  "version": "1.0.0",
  "description": "Internet speed tester application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . internet-speed-monitor --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . internet-speed-monitor --overwrite --asar --platform=win32 --arch=x64 --icon=resources/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Internet speed monitor\"",
    "package-linux": "electron-packager . internet-speed-monitor --overwrite --asar --platform=linux --arch=x64 --icon=resources/icon.png --prune=true --out=release-builds",
    "complete-package-linux": "npm run package-linux && node copyStaticAssets.js",
    "complete-package-win": "npm run package-win && node copyStaticAssetsWin32.js"
  },
  "repository": "https://github.com/Rigo85/internet-speed-monitor.git",
  "keywords": [],
  "author": "Rigoberto L. Salgado Reyes",
  "license": "AGPL-3.0",
  "devDependencies": {
    "electron": "^19.0.9",
    "electron-packager": "^15.5.1",
    "shelljs": "^0.8.5"
  },
  "dependencies": {
    "electron-log": "^4.4.8",
    "sqlite3": "^5.0.10",
    "universal-speedtest": "^2.0.5"
  }
}
